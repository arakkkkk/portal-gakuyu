	        </div></div><br>
	        <div class="col_name">
	        	<a class="aaa">備品：<%= rental_equip.name %></a>
	        </div>
	        <div class="col_name"><div class="btn2">
	        	<td><%= link_to '変更', kyokuin2_edit_path(rental_equip) %></td>
	        	</div><div class="btn2">
	        	<td><%= link_to '返却', kyokuin2_henkyaku_path(rental_equip) %></td>
	        	</div><div class="btn2">
	        	<tr>
		        <td><%= link_to 'Destroy', deletedata_path(rental_equip), :method => :delete, data: { confirm: 'Are you sure?' } %></td>
			    </tr>
	        </div></div><br>
	        <% end %>
    	</div>


    	</article>
    </section>
</div>
